{
  "name": "ngrx-rtk-query-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --ci --runInBand",
    "e2e": "ng e2e",
    "lint": "ng lint && stylelint '*/**/*.scss' --syntax scss",
    "format": "prettier --check --ignore-unknown .",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node .husky/hooks/pre-commit.js",
    "commit": "git add . && cz",
    "deploy": "ng deploy --base-href=/ngrx-rtk-query/",
    "copy": "cpx README.md dist/ngrx-rtk-query",
    "build:lib": "ng build ngrx-rtk-query && npm run copy",
    "test:lib": "jest --config ./projects/ngrx-rtk-query/jest.config.js",
    "test:lib:watch": "npm run test:lib -- --watch",
    "test:lib:coverage": "npm run test:lib -- --coverage",
    "test:lib:ci": "npm run test:lib -- --ci --runInBand",
    "postbuild:lib": "npm run build --prefix projects/ngrx-rtk-query",
    "semantic-release": "semantic-release",
    "update:msw": "npx msw init src --no-save",
    "prepare": "husky install",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.2",
    "@angular/common": "~12.1.2",
    "@angular/compiler": "~12.1.2",
    "@angular/core": "~12.1.2",
    "@angular/forms": "~12.1.2",
    "@angular/platform-browser": "~12.1.2",
    "@angular/platform-browser-dynamic": "~12.1.2",
    "@angular/router": "~12.1.2",
    "@ngneat/lib": "^2.0.1",
    "@ngrx/effects": "^12.2.0",
    "@ngrx/router-store": "^12.2.0",
    "@ngrx/store": "^12.2.0",
    "@reduxjs/toolkit": "^1.6.1",
    "rxjs": "~7.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^12.1.0",
    "@angular-devkit/build-angular": "^12.1.2",
    "@angular-eslint/builder": "^12.3.1",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.3.1",
    "@angular/cli": "~12.1.2",
    "@angular/compiler-cli": "~12.1.2",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@ngrx/store-devtools": "^12.2.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@tailwindcss/forms": "^0.3.3",
    "@testing-library/angular": "^10.10.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^13.2.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.16",
    "@types/node-fetch": "^2.5.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "all-contributors-cli": "^6.20.0",
    "angular-cli-ghpages": "1.0.0-rc.2",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-jsdoc": "^35.5.0",
    "eslint-plugin-ngrx": "^1.27.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-rxjs": "^3.3.5",
    "eslint-plugin-testing-library": "^4.9.3",
    "git-cz": "^4.7.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-preset-angular": "^9.0.4",
    "jsonc-parser": "^3.0.0",
    "lint-staged": "^11.0.1",
    "msw": "^0.32.0",
    "ng-packagr": "^12.1.2",
    "node-fetch": "^2.6.1",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "semantic-release": "^17.4.4",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^27.0.3",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,html}": "eslint --cache --fix",
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{scss,css}": [
      "stylelint --syntax scss --fix"
    ]
  }
}
