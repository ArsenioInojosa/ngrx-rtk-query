{
  "name": "ngrx-rtk-query-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --ci --runInBand",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "format": "prettier --check --ignore-unknown .",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node .husky/hooks/pre-commit.js",
    "commit": "git add . && cz",
    "deploy": "ng deploy --base-href=/ngrx-rtk-query/",
    "copy": "cpx README.md dist/ngrx-rtk-query",
    "build:lib": "ng build ngrx-rtk-query && npm run copy",
    "test:lib": "jest --config ./projects/ngrx-rtk-query/jest.config.js",
    "test:lib:watch": "npm run test:lib -- --watch",
    "test:lib:coverage": "npm run test:lib -- --coverage",
    "test:lib:ci": "npm run test:lib -- --ci --runInBand",
    "postbuild:lib": "npm run build --prefix projects/ngrx-rtk-query",
    "semantic-release": "semantic-release",
    "update:msw": "npx msw init src --no-save",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.0.3",
    "@angular/common": "~16.0.3",
    "@angular/compiler": "~16.0.3",
    "@angular/core": "~16.0.3",
    "@angular/forms": "~16.0.3",
    "@angular/platform-browser": "~16.0.3",
    "@angular/platform-browser-dynamic": "~16.0.3",
    "@angular/router": "~16.0.3",
    "@ngneat/lib": "~5.0.0",
    "@ngrx/effects": "~16.0.0",
    "@ngrx/router-store": "~16.0.0",
    "@ngrx/store": "~16.0.0",
    "@reduxjs/toolkit": "~1.9.5",
    "rxjs": "~7.8.1",
    "tslib": "~2.5.2",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "~16.0.0-beta.3",
    "@angular-devkit/build-angular": "~16.0.3",
    "@angular-eslint/builder": "~16.0.2",
    "@angular-eslint/eslint-plugin": "~16.0.2",
    "@angular-eslint/eslint-plugin-template": "~16.0.2",
    "@angular-eslint/schematics": "~16.0.2",
    "@angular-eslint/template-parser": "~16.0.2",
    "@angular/cli": "~16.0.3",
    "@angular/compiler-cli": "~16.0.3",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@ngrx/store-devtools": "~16.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/angular": "^14.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/events": "^3.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^16.18.33",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "all-contributors-cli": "^6.25.1",
    "angular-cli-ghpages": "1.0.6",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsdoc": "^44.2.7",
    "eslint-plugin-ngrx": "^2.1.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-rxjs": "^5.0.3",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-preset-angular": "~13.1.1",
    "jsonc-parser": "^3.2.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "ng-packagr": "~16.0.1",
    "node-fetch": "^2.6.11",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-attributes": "0.0.5",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "protractor": "~7.0.0",
    "semantic-release": "^21.0.2",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-scss": "^5.0.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,html}": "eslint --cache --fix",
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{scss,css}": [
      "stylelint --fix"
    ]
  }
}
