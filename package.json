{
  "name": "ngrx-rtk-query-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --prod",
    "test": "ng test --pass-with-no-tests",
    "e2e": "ng e2e",
    "lint": "ng lint && stylelint '*/**/*.scss' --syntax scss",
    "format": "prettier --check --ignore-unknown .",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "cz",
    "deploy": "ng deploy --no-build --base-href=https://SaulMoro.github.io/ngrx-rtk-query/",
    "copy": "cpx README.md dist/ngrx-rtk-query",
    "build:lib": "ng build ngrx-rtk-query --prod && npm run copy",
    "test:lib": "ng test ngrx-rtk-query --pass-with-no-tests",
    "test:lib:headless": "cross-env CI=true npm run test:lib",
    "postbuild:lib": "npm run build --prefix projects/ngrx-rtk-query",
    "semantic-release": "semantic-release",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "@ngneat/lib": "^2.0.0",
    "@ngrx/effects": "^11.0.1",
    "@ngrx/router-store": "^11.0.1",
    "@ngrx/store": "^11.0.1",
    "@reduxjs/toolkit": "^1.5.0",
    "@rtk-incubator/rtk-query": "https://pkg.csb.dev/rtk-incubator/rtk-query/commit/97413bf7/@rtk-incubator/rtk-query",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.1.1",
    "@angular-devkit/build-angular": "~0.1102.4",
    "@angular-eslint/builder": "2.0.1",
    "@angular-eslint/eslint-plugin": "2.0.1",
    "@angular-eslint/eslint-plugin-template": "2.0.1",
    "@angular-eslint/schematics": "2.0.1",
    "@angular-eslint/template-parser": "2.0.1",
    "@angular/cli": "~11.2.4",
    "@angular/compiler-cli": "~11.2.5",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@ngrx/store-devtools": "^11.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@tailwindcss/forms": "^0.2.1",
    "@testing-library/angular": "^10.4.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "26.0.20",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "all-contributors-cli": "^6.20.0",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsdoc": "32.2.0",
    "eslint-plugin-ngrx": "^1.17.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-rxjs": "^3.1.1",
    "eslint-plugin-testing-library": "^3.10.1",
    "git-cz": "^4.7.6",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "jsonc-parser": "^3.0.0",
    "lint-staged": "^10.5.4",
    "msw": "^0.27.1",
    "ng-packagr": "^11.2.4",
    "postcss": "^8.2.8",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "semantic-release": "^17.4.2",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "tailwindcss": "^2.0.3",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,html}": "eslint --cache --fix",
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write  --ignore-unknown"
    ],
    "*.{scss,css}": [
      "stylelint --syntax scss --fix"
    ]
  }
}
