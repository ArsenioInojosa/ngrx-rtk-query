{
  "name": "ngrx-rtk-query",
  "version": "1.0.0",
  "description": "Make RTK Query works in Angular Applications",
  "peerDependencies": {
    "typescript": "^4.1.0",
    "@angular/core": ">= 11.0.0 < 14",
    "@ngrx/store": ">= 11.0.0 < 14",
    "rxjs": ">= 6.5.3 < 8",
    "@reduxjs/toolkit": "~1.7.1"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "keywords": [
    "angular",
    "angular rtk query",
    "ngrx-rtk-query",
    "ngrx rtk query",
    "rtk-query",
    "ngrx",
    "redux",
    "redux-toolkit"
  ],
  "author": "Saul Moro",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/SaulMoro/ngrx-rtk-query/issue"
  },
  "homepage": "https://github.com/SaulMoro/ngrx-rtk-query#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/SaulMoro/ngrx-rtk-query"
  },
  "schematics": "./schematics/collection.json",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "copy:schemas": "cpx schematics/ng-add ../../dist/ngrx-rtk-query/",
    "copy:collection": "cpx schematics/collection.json ../../dist/ngrx-rtk-query/schematics/",
    "postbuild": "npm run copy:schemas && npm run copy:collection"
  }
}
